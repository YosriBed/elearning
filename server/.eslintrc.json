{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "max-len": "off",
    "consistent-return": "off",
    "no-console": "off",
    "no-underscore-dangle": "off",
    "func-names": "off"
  }
}
